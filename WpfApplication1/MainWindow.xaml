        <Label Content="4" Height="28" HorizontalAlignment="Left" Margin="437,29,0,0" Name="lblStep4" VerticalAlignment="Top" MouseDoubleClick="DoubleClickStep4" />
        <Label Content="5" Height="28" HorizontalAlignment="Left" Margin="459,29,0,0" Name="lblStep5" VerticalAlignment="Top" MouseDoubleClick="DoubleClickStep5" />
